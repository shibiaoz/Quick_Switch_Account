{
    "manifest_version": 2,
    "name": "Teiba_Fe_Helper",
    "version": "1.0",
    "description": "快速切换账号！一些小功能的集成",
    "background": {
        "scripts": [
            "js/background.js"
        ],
        "persistent": false
    },

    "page_action": {
       "default_icon": "icon.png",
       "default_title": "22Teiba_Fe_Helper ",    // optional; shown in tooltip
       "default_popup": "page_popup.html"  
    },

    /*"browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
      },*/
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },

    

    "content_scripts": [
        {
            "matches": ["http://*.baidu.com/*"],
            "js": ["js/content.js"]
        }
    ],
    "web_accessible_resources": [ "js/extensionInsertInto.js" ],
    "permissions": [
        "contextMenus",
        "activeTab",
        "downloads",
        "tabs",
        "http://tieba.baidu.com/",
        "declarativeContent",
        "storage"
    ]
}
